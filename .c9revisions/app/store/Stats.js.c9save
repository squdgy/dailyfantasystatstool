{"ts":1359297114829,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"/*\n\nCopyright (c) 2012 Maura Wilder\n\n*/\nExt.define('DFST.store.Stats', {\n    extend: 'Ext.data.Store',\n\n    requires: ['Ext.data.reader.Json'],\n\n    model: 'DFST.model.StatSet',\n\n    autoLoad: false,\n    pageSize: 20,\n    proxy: {\n\t\ttype: 'ajax',\n        url: 'app/data/stats.json', //test data, url overridden in controller\n\t\treader: {\n\t\t\ttype: 'json',\n            root: 'players',\n            totalProperty: 'total'            \n\t\t},\n        sorters: {\n            property : 'afp',\n            direction: 'DESC'\n        },\n        encodeSorters: function(sorters) {\n            // ASP.Net WEB API can't handle the json-ized sort url param?\n             var length   = sorters.length,\n                 sortStrs = [],\n                 sorter, i;\n    \n             for (i = 0; i < length; i++) {\n                 sorter = sorters[i];\n    \n                 sortStrs[i] = sorter.property + '#' + sorter.direction;\n             }\n\n            return sortStrs.join(\",\");\n         },\n        encodeFilters: function(filters) {\n            // ASP.Net WEB API can't handle the json-ized sort url param?\n             var length   = filters.length,\n                 str = [],\n                 filter, i;\n    \n             for (i = 0; i < length; i++) {\n                 filter = filters[i];\n    \n                 str[i] = filter.property + '#' + filter.value;\n             }\n\n            return str.join(\",\");\n         }         \n         \n\t},\n    remoteSort: true,\n    remoteFilter: true\n});\n\n"]],"start1":0,"start2":0,"length1":0,"length2":1484}]],"length":1484}
